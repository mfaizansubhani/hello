    "import re\n",
    "x=input(\"Enter File Name: \")\n",
    "fhand= open(x+\".txt\")\n",
    "l=list()\n",
    "x=int(0)\n",
    "for line in fhand:\n",
    "    l.extend(re.findall(r\"[\\d]+\",line))\n",
    "for lpc in l:\n",
    "    x+=int(lpc)\n",
    "print(x/len(l))"
   ]
